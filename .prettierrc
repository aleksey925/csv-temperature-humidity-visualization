{
    "printWidth": 120,
    "tabWidth": 3,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "jsxBracketSameLine": true,
    "arrowParens": "avoid",
    "proseWrap": "always",
    "endOfLine": "lf"
}
